                    <script>
                        // وظيفة عامة لإخفاء أي رسالة بعد مدة
                        function hideMessage(id, timeout = 5000) {
                            setTimeout(function () {
                                var msg = document.getElementById(id);
                                if (msg) {
                                    msg.style.transition = "opacity 0.5s";
                                    msg.style.opacity = "0";
                                    setTimeout(() => msg.remove(), 500);
                                }
                            }, timeout);
                        }

                        // نطبقها على الرسائل الموجودة
                        hideMessage("Message", 4000); // 4 ثواني للنجاح
                        hideMessage("Error", 6000);   // 6 ثواني للخطأ
                    </script>
                    @RenderBody()

